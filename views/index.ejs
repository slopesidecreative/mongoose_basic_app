<!DOCTYPE html>
<html>
<head>
   <meta charset="utf-8">
   <title></title>
<script type ="text/javascript" src="/js/jquery-3.1.1.js"></script>
<script type ="text/javascript" src="/js/main.js"></script>
</head>
<body>

<div id='results'>
   <ul>
      <% if(typeof users == 'object' && users){ %>
         <% for(user in users){ %>
         <li><%= users[user].name %></li>
         <li><%= users[user].age %></li>
         <% } %>
      <% } %>
   </ul>
</div>


<div>
   <form id="user" action="/users" method="post">
      <input type="text" name="name"><br/>
      <input type="text" name="age"><br/>
      <button class='user_submit'>submit</button>
   </form>
</div>
<div>
</div>
</body>

</html>
